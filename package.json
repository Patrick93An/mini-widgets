{
  "name": "mini-widgets",
  "version": "1.0.0",
  "description": "Collection of lightweight mini widgets or apps.",
  "main": "index.js",
  "author": "Patrick Low",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --inline --open --content-base ./dist/ ./dist/app/bundle.js --port 3000",
    "build": "webpack --watch --mode development ./src/app/index.js --output ./dist/app/bundle.js",
    "build:prod": "webpack --mode production ./src/app/index.js --output ./dist/app/bundle.js"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  }
}
